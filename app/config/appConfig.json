{ 
  "mapConfig" : "app/config/mapConfig.json",
  "layersConfig" : "app/config/layersConfig.json",  
  "columnGroups" : "app/config/columnGroups.json",
  "columns" : "app/config/columns.json",  
  "labels" : "app/config/labels.json",
  "records":{    
    "model" : "geojson",
    "path" : "app/data/tsunamidb.geojson",  
    "featureAttributeMap" : {
      "id" : "event_id"
    },
    "layerOptions":{          
      "type":"point",      
      "wrap" : true,
      "style" : {
        "color":"red",
        "weight": 1,
        "opacity" : 1,
        "fillOpacity" : 0.3,
        "radius":6
      }
    }
  },
  "proxies":{    
    "path" : "app/data/proxies.geojson",  
    "featureAttributeMap" : {
      "id" : "proxy_id"      
    }
  },
  "references":{    
    "path" : "app/data/references.geojson"  
  }  
}  
